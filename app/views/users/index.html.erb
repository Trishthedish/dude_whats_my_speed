<h1>HELLO USER</h1>


<% @users.each do |user| %>
  <h1>Name: <%= user.name %></h1>
  <h3>Service Provider in Question: <%= user.service_provider %></h3>
  <ul>
    <li>Monthly Cost: <%= user.monthly_cost %></li>
    <li> Paid Download Speed: <%= user.paid_download %></li>
    <li> Paid Upload Speed: <%= user.paid_upload %></li>
    <li> Area Code: <%= user.area_code %></li>
    <li> Created at: <%=user.created_at %></li>
  </ul>
<% end %>


<div class="row">
  <!-- Line Chart - Multiple Series -->
  <div class="col-xs-6">
    <h3> NAME OF GRAPH</h3>
    <%= line_chart RasberryPi.group(:ping).count  %>
    <hr>


  </div>



</div>
  <%= area_chart @pi_data.group_by_minute(:created_at).count(:ping) %>
