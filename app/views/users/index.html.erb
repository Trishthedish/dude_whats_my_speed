<h1>HELLO USER</h1>




<h2><%= @pi_data.count %></h2>

<%= @pi_data.each do |data| %>
<ul>
  <li><%= data.created_at %></li>
  <li><%= data.ping %></li>
  <li><%= data.download %></li>
  <li><%= data.upload %></li>
</ul>

<% end %>

<%= line_chart [
  {name: "Ping", data: @pi_data.map {|p| p.ping}},
  {name: "Download", data: @pi_data.map {|d| d.download}},
  {name: "Upload", data: @pi_data.map{|u| u.upload}}


] %>
